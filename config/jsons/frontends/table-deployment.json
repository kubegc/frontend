{
    "apiVersion": "doslab.io/v1",
    "kind": "Frontend",
    "metadata": {
        "name": "table-apps.deployment",
        "namespace": "default"
    },
    "spec": {
        "data": [
            {
                "key": 1,
                "kind": "internalLink",
                "label": "deployment名",
