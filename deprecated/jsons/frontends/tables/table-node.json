{
	"apiVersion": "doslab.io/v1",
	"kind": "FrontendTable",
	"metadata": {
		"name": "table-node",
		"namespace": "default"
	},
	"spec": [{
			"kind": "externalLink",
			"label": "主机名",
			"link": "",
			"row": "metadata.name",
			"type": "input"
		},
		{
			"kind": "internalLink",
			"label": "运行实例",
			"link": "Pod",
			"row": "@metadata.name",
			"tag": "spec#nodeName",
			"type": "select"
		},
		{
			"label": "IP地址",
			"row": "status.addresses[0].address"
		},
		{
			"label": "CPU",
			"row": "status.capacity.cpu"
		},
		{
			"label": "内存",
			"row": "status.capacity.memory"
		},
		{
			"label": "最大实例数",
			"row": "status.capacity.pods",
			"width": "120"
		},
		{
			"label": "操作系统",
			"row": "status.nodeInfo.osImage"
		},
		{
			"label": "创建时间",
			"row": "status.conditions[0].lastHeartbeatTime"
		},
		{
			"kind": "terminal",
			"label": "远程连接",
			"link": "http://39.106.40.190:31051/ssh/host/{1}",
			"row": "status.addresses[0].address"
		},
		{
			"kind": "action",
			"label": "更多操作",
			"row": ""
		}
	]
}
