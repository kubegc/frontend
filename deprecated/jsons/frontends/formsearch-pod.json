{
  "metadata": {
    "name": "formsearch-pod",
    "namespace": "default"
  },
  "apiVersion": "doslab.io/v1",
  "kind": "FrontendFormseach",
  "spec": {
    "data": {
      "items": [{
        "name": "资源名称:",
        "path": "metadata#name",
        "style": "display:inline-block;margin-right:10px",
        "type": "textbox",
        "default": "默认值"
      },
        {
          "name": "所在主机:",
          "path": "spec#nodeName",
          "style": "display:inline-block;margin-right:10px",
          "type": "combobox",
          "default": "keys(metadata#labels)"
        }
      ],
      "submitButton": "搜索",
      "resetButton": "重置信息"
    },
    "type": "formsearch"
  }
}
