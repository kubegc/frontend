                                "name": "string",
                                "namespace": "string"
                                  },
                                "spec": {
                                "selector": {
                                "matchLabels": {
                                "job-name": "test-job"
                            }
                             },
                                "template": {
                                "metadata": {
                                "creationTimestamp": null,
                                "labels": {
                                "job-name": "test-job"
                          }
                            },
                                "spec": {
                                "containers": [
                            {
                                "command": [
                                  "string"
                        ],
                        "image": "string",
                        "imagePullPolicy": "Always",
                        "name": "test-job"
                    }
                ]
            }
        }
    }

                        }
		}
	}
